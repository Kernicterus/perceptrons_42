{
    "network_1": {
        "input_layer": {
            "activation": "sigmoid"
        },
        "hidden_layer_1": {
            "neurons": 2,
            "activation": "sigmoid",
            "weights_init": "heUniform"
        },
        "hidden_layer_2": {
            "neurons": 3,
            "activation": "sigmoid",
            "weights_init": "heUniform"
        },
        "hidden_layer_3": {
            "neurons": 4,
            "activation": "sigmoid",
            "weights_init": "heUniform"
        },
        "output_layer": {
            "activation" : "softmax",
            "weights_init" : "heUniform"
        }
    },
    "model_fit" : {
        "network" : "network_1",
        "loss" : "binaryCrossEntropy",
        "learning_rate" : 0.01,
        "batch_size" : 32,
        "epochs" : 9
    }
}